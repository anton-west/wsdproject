<h1>Report!</h1>

<h2>Morning!</h2>

<form method="POST">

    <p>date</p>

    <input type="date" name="date"/>

    <p>sleep duration</p>
    
    <input type="number" name="sleep duration" min = "0" step="0.1"/>
    
    <p>sleep quality</p>
    
    <input type="number" name="sleep quality" min = "1" max = "5"/>
    
    <p>general mood</p>
    
    <input type="number" name="general mood" min = "1" max = "5"/>
    
    <input type="submit" value="Submit!" />

</form>

<h2>Evening!</h2>

<form method="POST">

    <p>date</p>

    <input type="date" name="date"/>

    <p>time spent on sports and exercise</p>
    
    <input type="number" name="sport duration" min = "0" step="0.1"/>
    
    <p>time spent studying</p>
    
    <input type="number" name="study duration" min = "0" step="0.1"/>

    <p>regularity of eating</p>
    
    <input type="number" name="eating regularity" min = "1" max = "5"/>

    <p>quality of eating</p>
    
    <input type="number" name="eating quality" min = "1" max = "5"/>

    <p>general mood</p>
    
    <input type="number" name="general mood" min = "1" max = "5"/>
    
    <input type="submit" value="Submit!" />

</form>
